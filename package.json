{
  "name": "ai-chatbot",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "tsx -r dotenv/config lib/db/migrate dotenv_config_path=.env.local && next build",
    "start": "next start",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx lib/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:seed": "npx tsx scripts/seed-companies.ts",
    "test": "vitest run --coverage",
    "test:document-upload": "npx tsx scripts/test-document-upload.ts",
    "typecheck": "tsc --noEmit",
    "pre-deploy": "tsx scripts/pre-deploy-check.ts",
    "prepare": "husky install",
    "validate-pow": "node scripts/validate-pow.js",
    "claude:custom": "claude -p \"$(cat toolsets/claude_custom_instructions.jsonc)\"",
    "fix-auth": "tsx -r dotenv/config scripts/fix-auth-emergency.ts dotenv_config_path=.env.local",
    "reset-auth": "tsx -r dotenv/config scripts/reset-auth.ts dotenv_config_path=.env.local",
    "create-admin": "tsx -r dotenv/config scripts/create-platform-admin.ts dotenv_config_path=.env.local",
    "list-users": "tsx -r dotenv/config scripts/list-users.ts dotenv_config_path=.env.local"
  },
