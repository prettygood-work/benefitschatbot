{
  "name": "ai-chatbot",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo --hostname 0.0.0.0",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 next build",
    "start": "next start",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "format": "biome format --write",
    "test": "tsx vitest run --coverage",
    "test:document-upload": "tsx scripts/test-document-upload.ts",
    "typecheck": "NODE_OPTIONS=--max-old-space-size=8192 tsc --noEmit",
    "pre-deploy": "tsx scripts/pre-deploy-check.ts",
    "prepare": "husky install",
    "validate-pow": "node scripts/validate-pow.js",
    "claude:custom": "claude -p \"$(cat toolsets/claude_custom_instructions.jsonc)\"",
    "fix-auth": "tsx -r dotenv/config scripts/fix-auth-emergency.ts dotenv_config_path=.env.local",
    "reset-auth": "tsx -r dotenv/config scripts/reset-auth.ts dotenv_config_path=.env.env.local",
    "create-admin": "tsx -r dotenv/config scripts/create-platform-admin.ts dotenv_config_path=.env.local",
    "list-users": "tsx -r dotenv/config scripts/list-users.ts dotenv_config_path=.env.env.local",
    "assign-super-admin": "tsx -r dotenv/config scripts/assign-super-admin.ts dotenv_config_path=.env.local",
    "validate-env": "tsx scripts/validate-env.ts",
    "uat-tests": "tsx -r dotenv/config scripts/uat-tests.ts dotenv_config_path=.env.local"
  },
